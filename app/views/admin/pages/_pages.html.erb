<%= render_void_table(@pages.size, 5)  %>

<% for page in @pages %>
<tr>
  <td>
    <%= link_to_edit_with_profiles page.title, page %>
    <%= show_actions page %>
  </td>
  <td><%= page.name%></td>
  <td>
    <%= author_link(page) %><br />
    <small><%= format_date page.created_at %></small>
  </td>
</tr>
<% end %>
<%= display_pagination(@pages, 5)%>
